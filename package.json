    "@ts-overflow/node-framework": "^1.0.1",
    "ndforge": "^1.5.3",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.3",
    "sass": "^1.85.0",
